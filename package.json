{
  "name": "linter-packer-validate",
  "main": "./lib/main.js",
  "version": "1.6.1",
  "description": "Packer lint via validate and format via fmt",
  "repository": "https://github.com/mschuchard/linter-packer-validate",
  "license": "MIT",
  "keywords": [
    "lint",
    "linter",
    "packer"
  ],
  "scripts": {
    "test": "pulsar --test spec"
  },
  "bugs": {
    "url": "https://github.com/mschuchard/linter-packer-validate/issues"
  },
  "engines": {
    "pulsar": ">=1.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "readmeFilename": "README.md",
  "dependencies": {
    "atom-linter": "^10.0.0"
  },
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-airbnb-base": "latest",
    "eslint-plugin-import": "latest"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "pulsar"
          ]
        }
      ]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "pulsar": false
    }
  }
}
